[Unit]
Description="{{ brik_name }}-redis-socketio"
PartOf={{ brik_name }}-redis.target

[Service]
User={{ user }}
Group={{ user }}
Restart=always
ExecStart={{ redis_server }} {{ redis_socketio_config }}
StandardOutput=file:{{ brik_dir }}/logs/redis-socketio.log
StandardError=file:{{ brik_dir }}/logs/redis-socketio.error.log
WorkingDirectory={{ sites_dir }}
